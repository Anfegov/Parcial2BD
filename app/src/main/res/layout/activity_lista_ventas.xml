<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ListaVentasActivity"
    android:background="@drawable/ic_bikini">


    <LinearLayout
        android:layout_width="372dp"
        android:layout_height="514dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="4dp"
        tools:layout_editor_absoluteY="0dp">

        <FrameLayout
            android:id="@+id/id_FrameLayout_Principal3"
            android:layout_width="match_parent"
            android:layout_height="416dp">

        </FrameLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">



            <ImageButton
                android:id="@+id/button_listar_ventas"
                android:layout_width="55dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="#10000000"
                app:srcCompat="@mipmap/ic_launcher_lista_ventas_round" />

            <ImageButton
                android:id="@+id/button_buscar_venta"
                android:layout_width="55dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="#10000000"
                app:srcCompat="@mipmap/ic_launcher_buscar_round" />


            <ImageButton
                android:id="@+id/button_regresar"
                android:layout_width="55dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="#10000000"
                app:srcCompat="@mipmap/ic_launcher_regresar_round" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="64dp"
            android:background="#10000000"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView2"
                android:layout_width="55dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="Listar Ventas"
                android:textColor="#CF3476"
                android:textAlignment="center" />

            <TextView
                android:id="@+id/textView3"
                android:layout_width="55dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="Buscar Venta"
                android:textColor="#CF3476"
                android:textAlignment="center" />


            <TextView
                android:id="@+id/textView7"
                android:layout_width="55dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="Regresar"
                android:textColor="#CF3476"
                android:textAlignment="center" />
        </LinearLayout>

    </LinearLayout>




</android.support.constraint.ConstraintLayout>